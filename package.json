{
  "name": "console-1",
  "version": "1.0.0",
  "description": "A terminal wrapper with Claude Code integration",
  "main": "dist/main/main/index.js",
  "scripts": {
    "start": "electron .",
    "start:cli": "ts-node src/index.ts",
    "build": "tsc",
    "build:main": "tsc -p tsconfig.main.json",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build:renderer": "tsc -p tsconfig.renderer.json",
    "build:all": "npm run build:main && npm run build:preload && npm run build:renderer",
    "dev": "npm run build:all && electron .",
    "dev:cli": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.1",
    "node-pty": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "electron": "^28.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
